<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">
        
        <!-- Default Payment Provider (disabled by default) -->
        <record id="payment_provider_vipps_mobilepay" model="payment.provider">
            <field name="name">Vipps/MobilePay</field>
            <field name="code">vipps</field>
            <field name="state">disabled</field>
            <!-- Set official MobilePay/Vipps logo on the provider record -->
            <field name="image_1920" type="base64" file="static/description/MP_logo_badge.svg"/>
            <field name="image_128" type="base64" file="static/description/MP_logo_badge.svg"/>
        </record>
        
    </data>
</odoo>